<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <title>Codenames</title>
        <link rel="stylesheet" href="/css/bootstrap-5.0-min.css">
        <link rel="stylesheet" href="/css/custom.css">
    </head>


    <body>

    <%- include("header") %>
    <div id="doc">
        <main class="container-fluid">

            <div class="text-center w-25 mx-auto">
                <h1 class="h3 mb-3 fw-normal">Please sign in</h1>
                <h2 id="err"></h2>
                <input type="email" id="log" class="form-control" placeholder="Email address" required="" autofocus="">
                <input type="password" id="pas" class="form-control" placeholder="Password" required="">
                <button id="send" class="w-100 btn btn-lg btn-primary" type="submit">Sign in</button>
            </div>
         </main>
    </div>


    </body>

    <script src="/js/ajax.js"></script>
    <script src="/js/register.js"></script>

</html>